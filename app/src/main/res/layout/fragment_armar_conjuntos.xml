<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ArmarConjuntosFragment">

    <FrameLayout
        android:id="@+id/contenedorCarrusel1"
        android:layout_width="wrap_content"
        android:layout_height="230dp"
        android:layout_above="@+id/contenedorCarrusel2"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="5dp" />

    <FrameLayout
        android:id="@+id/contenedorCarrusel2"
        android:layout_width="wrap_content"
        android:layout_height="230dp"
        android:layout_above="@+id/contenedorCarrusel3"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="58dp"
        android:layout_marginBottom="5dp" />

    <FrameLayout
        android:id="@+id/contenedorCarrusel3"
        android:layout_width="wrap_content"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="60dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="5dp" />
</RelativeLayout>